{
    "name": "basic-demo",
    "version": "0.0.1",
    "private": true,
    "main": "index.js",
    "module": "index.js",
    "scripts": {
        "build": "tsc --build",
        "build:watch": "tsc --build --watch",
        "serve": "web-dev-server --node-resolve --app-index index.html",
        "serve:watch": "web-dev-server --node-resolve --watch --open --app-index index.html",
        "dist": "rimraf ../dist/basic && rollup -c rollup.config.js"
    },
    "dependencies": {
        "@wctoolkit/basic": "*"
    },
    "devDependencies": {
        "tsconfig": "*",
        "serveconfig": "*",
        "rollupconfig": "*"
    }
}